import java.util.*;

public class BigWord {
	public String most(String[] sentences){
		Map<String, Integer> words = new TreeMap<String, Integer>();
		String blank = "";
		for(int i = 0; i < sentences.length; i++){
			blank += sentences[i] + " ";
		}
		sentences = blank.split();

		for(int i = 0; i < sentences.length; i++){
			if(words.containsKey(sentences[i])){
				
			}
		}
	}
}
